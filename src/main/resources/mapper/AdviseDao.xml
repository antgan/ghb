<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 目的：为DAO接口方法提供sql配置 -->
<mapper namespace="com.soecode.ghb.dao.AdviseDao">
	<insert id="insert" parameterType="Advise">
		insert into advise(advise, name ,email, visit_ip) values(#{advise}, #{name} ,#{email},#{visit_ip}) 
	</insert>
</mapper>